{
  "name": "fishlinic",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:ui": "next dev -p 3000",
    "dev:bridge": "tsx mock-server/index.ts",
    "dev": "run-p dev:*",
    "build": "prisma generate && next build",
    "build:prod": "cross-env NODE_ENV=production prisma generate && next build",
    "start": "next start",
    "start:prod:all": "run-p start:ui:prod start:bridge:prod",
    "start:ui:prod": "cross-env NODE_ENV=production next start -p 3000",
    "start:bridge:prod": "cd mock-server && npm run start:prod",
    "start:bridge:dev": "tsx mock-server/index.ts",
    "start:bridge": "cd mock-server && npm start",
    "start:core": "run-p start:ui:prod start:bridge:prod",
    "exhibition": "npm run build:production && npm run start:core",
    "lint": "eslint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "@prisma/client": "^6.19.0",
    "@serialport/parser-readline": "^13.0.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@vercel/analytics": "^1.6.1",
    "dotenv": "^17.2.2",
    "echarts": "^5.5.0",
    "echarts-for-react": "^3.0.2",
    "express": "^5.1.0",
    "lucide-react": "^0.548.0",
    "next": "15.5.3",
    "next-auth": "^4.24.13",
    "next-themes": "^0.4.6",
    "prisma": "^6.19.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-qr-code": "^2.0.12",
    "serialport": "^13.0.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/express": "^5.0.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
